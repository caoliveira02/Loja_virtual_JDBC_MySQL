package br.com.jdbc.modelo;

public class Produto {
	
	public Integer id;
	public String nome;
	public String descricao;
	
	public Produto(String nome, String descricao) {
		super();
		this.nome = nome;
		this.descricao = descricao;
	}

	public String getNome() {
		return nome;
	}

	public String getDescricao() {
		return descricao;
	}


	
}
